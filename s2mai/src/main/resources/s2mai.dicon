<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN"
	"http://www.seasar.org/dtd/components23.dtd">
<components namespace="s2mai" >
	<include path="mailProperties.dicon"/>
	<component name="s2MaiInterceptor" class="org.seasar.mai.interceptors.S2MaiInterceptor"/>
	<component class="org.seasar.mai.meta.impl.MaiMetaDataFactoryImpl"/>
	<component class="org.seasar.mai.annotation.impl.AnnotationReaderFactoryImpl"/>
	
	<component class="org.seasar.mai.property.impl.PropertyWriterImpl" />	
	<component class="org.seasar.mai.property.mail.impl.MailPropertyWriterFactoryImpl" />
	<component class="org.seasar.mai.property.mail.impl.MailPropertyWriterFrom" />
	<component class="org.seasar.mai.property.mail.impl.MailPropertyWriterTo" />
	<component class="org.seasar.mai.property.mail.impl.MailPropertyWriterCc" />
	
	<component class="org.seasar.mai.mail.impl.SendMailImpl">
		<property name="host">host</property>
		<property name="port">port</property>
		<property name="username">username</property>
		<property name="password">password</property>
		<property name="returnPath">returnPath</property>
		<property name="messageId">messageId</property>
		<property name="connectionTimeout">connectionTimeout</property>
		<property name="readTimeout">readTimeout</property>
	</component>
</components>
